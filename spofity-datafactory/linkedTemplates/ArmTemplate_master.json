{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spofity-datafactory"
		},
		"spotify_Sql_Database_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'spotify_Sql_Database'"
		},
		"spotify_storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'spotify_storage'"
		},
		"spotify_Sql_Database_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifyproject-server.database.windows.net"
		},
		"spotify_Sql_Database_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotify-databse"
		},
		"spotify_Sql_Database_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin123"
		},
		"spotify_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifystoragepraj.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spofity-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"spotify_Sql_Database_password": {
						"value": "[parameters('spotify_Sql_Database_password')]"
					},
					"spotify_storage_accountKey": {
						"value": "[parameters('spotify_storage_accountKey')]"
					},
					"spotify_Sql_Database_properties_typeProperties_server": {
						"value": "[parameters('spotify_Sql_Database_properties_typeProperties_server')]"
					},
					"spotify_Sql_Database_properties_typeProperties_database": {
						"value": "[parameters('spotify_Sql_Database_properties_typeProperties_database')]"
					},
					"spotify_Sql_Database_properties_typeProperties_userName": {
						"value": "[parameters('spotify_Sql_Database_properties_typeProperties_userName')]"
					},
					"spotify_storage_properties_typeProperties_url": {
						"value": "[parameters('spotify_storage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}